font_size 9
force_ltr yes
font_family InputMono
italic_font InputMono
bold_italic_font InputMono
hide_window_decorations yes
map ctrl+shift+n toggle_fullscreen
window_padding_width 5 5

remember_window_size  no
initial_window_width  1100
initial_window_height 600

# BEGIN_KITTY_THEME
# Dracula
include current-theme.conf
# END_KITTY_THEME
